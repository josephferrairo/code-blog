<div class="col-md-11 centerizer booyah-box">

  <p class="text-center">
    <h2 class="text-center"> <%= @profile.user.full_name %> </h2>
  </p>

  <div class="text-center">
    <%= raw(@profile.biography) %>
  </div>

  <p class="text-center">
    <%= @profile.web_browser %>
  </p>

  <p class="text-center">
    <%= @profile.terminal %>
  </p>

  <p class="text-center">
    <%= @profile.text_editor %>
  </p>

  <%= link_to 'Back', profiles_path, class: "btn btn-danger" %>
</div>
